 /*				*/
.cb:after {content:'';display:block;clear:both}

.screen_out {display:block;position:absolute;left:-9999px;overflow:hidden;width:0;height:0;font-size:0;line-height:0;text-indent:-9999px}
.hidden {padding:0!important; height:0; font-size:0; line-height:0; overflow:hidden; visibility:hidden; border:0!important;}

.text_left {text-align:left!important;}
.text_right {text-align:right!important;}
.text_center {text-align:center!important;}
.text_middle {vertical-align:middle;}

.em {margin-top:25px;color:#ed5432!important;}
.strong {font-weight:700!important;}

.ellipsis {width:90%; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}

/* status color */
.upper {color:#ff07f0 !important;}
.up {color:#35970f !important;}
.same {color:#ffc000 !important;}
.low {color:#f5394f !important;}
.lower {color:#0acaf5 !important;}

/* watch list status color */
.up1 {color:#00ff00 !important;}
.same1 {color:#ffff00 !important;}
.low1 {color:#fb0a0a !important;}


.highlight {
    -moz-animation: highlight 2s ease 0s 1 alternate ;
    -webkit-animation: highlight 2s ease 0s 1 alternate;
    animation: highlight 2s ease 0s 1 alternate;
 }

.rehighlight {
    -moz-animation: rehighlight 2s ease 0s 1 alternate ;
    -webkit-animation: rehighlight 2s ease 0s 1 alternate;
    animation: rehighlight 2s ease 0s 1 alternate;
 }

@-webkit-keyframes highlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}
}
@-moz-keyframes highlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}
@-keyframes highlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}

@-webkit-keyframes rehighlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}
}
@-moz-keyframes rehighlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}
@-keyframes rehighlight {
    from { background-color: black;}
    to {background-color: #F6F6BC;}
}

.arrow:before {content:''; display:inline-block; margin-right:7px; border:transparent solid; }
.arrow.upper:before {width:9px;height:8px;background:url(/resources/images/arrow_upper.png) no-repeat;}
.arrow.up:before {border-width:0 4px 8px;border-bottom-color:#35970f;}
.arrow.same:before {margin-bottom:3px;border-width:1px 5px;border-color:#ffc000;}
.arrow.low:before {border-width:8px 4px 0px;border-top-color:#f5394f;}
.arrow.lower:before {width:9px;height:8px;background:url(/resources/images/arrow_lower.png) no-repeat;}

/* header */
header {position:relative; margin-bottom:15px; height:66px; }
header h1 {float:left; padding-top:26px;}
header .side {position:absolute; top:8px; right:0;}
header .side:after {content:''; display:block; clear:both;}
header .side .buttons {position:relative; float:right; margin-left:10px; padding:0 0 0 10px; top:-2px;}
header .side .buttons:after {content:''; position:absolute; top:4px; left:0; height:8px; border-left:1px solid #dfdfdf;}
header .side .buttons button {padding:0; background:none; border:0; text-decoration:underline;}
header .side .buttons .language {display:inline-block; padding:0 0 0 15px;}
header .side .buttons .language button {font-size:0; line-height:0; width:19px;}
header .side .buttons .language button.on {padding:1px; border:1px solid #2e66b1;}
header .side .buttons .language button img {width:15px;}
header .side .text {float:right; color:#888; line-height:12px;}
header .side .text span {position:relative; margin-left:6px; padding-left:10px;}
header .side .text span:after {content:''; position:absolute; top:2px; left:0; height:8px; border-left:1px solid #dfdfdf;}
header .side .text strong,
header .side .text em,
header .side .text .em {font-weight:400; color:#343434;}
header .side .wtsJisu {float:right;margin-right:0px;width:350px;height:25px;overflow:hidden;}
header .side .wtsJisu .index_area {padding:0px 0px 10px 10px;}

header .gnb {position:absolute; bottom:0; left:15%; display:table; width:85%;}
header .gnb:after {display:block;content:'';clear:both;float:none;}

/* gnb */
.wts_menu {overflow:hidden; display:table-row;}
.wts_menu li {/*float:left;*/ display:table-cell;}
.wts_menu li + li {border-left:1px solid #949aa1;}
.wts_menu li button {display:block; margin:0; padding:0 10px; width:100%; height:38px; color:#fff; font-weight:bold; border:0; background:#a5a9ad; letter-spacing:-0.02em; word-spacing:-0.02em; transition:color .2s, background .2s; text-align:center;}
.wts_menu li button:hover {color:#4973ad; background:#b5b5b5;}
.wts_menu li.on button {color:#fff; background:#4973ad;}

/* footer */
.footer {margin-top:15px; padding-top:6px; height:54px; border-top:1px solid #ddd;}
.footer .cb {float:right;}
.footer .news {overflow:hidden;}
.footer .news ul {float:left; width:310px; height:15px; line-height:15px; overflow:hidden;}
.footer .news .move_area {float:right;}
.footer .news button {float:left; padding:0; width:13px; height:13px; background:url(/resources/images/bg_arrow_sml.png) no-repeat; border:1px solid #d7d7d7; text-indent:-9999px;}
.footer .news button.btn_prev {background-position:4px 50%; border-radius:2px 0 0 2px;}
.footer .news button.btn_next {background-position:-43px 50%; border-radius:0 2px 2px 0; border-left:0;}
.footer .news button:hover {border-color:#aaa;}
.footer .news button.btn_next:hover {position:relative; left:-1px; border-left:1px solid #aaa;}
.footer .news_list a {display:block; width:300px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis;font-weight:bold;}
.footer .copy {margin-top:10px; text-align:right; color:#666;}

/* ticker bar */
.wrap_ticker {float:left;}
/*.wrap_ticker .index_area {float:left; width:285px; line-height:13px;}*/
.wrap_ticker .index_area {float:left; width:320px; line-height:15px;font-size:14px;}
.wrap_ticker .index_area:first-child {margin-right:30px;}
.wrap_ticker .index_area:after {display:block;content:'';clear:both;float:none;}
/*.wrap_ticker .index_area h4 {float:left; width:70px; font-weight:400; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}*/
.wrap_ticker .index_area h4 {float:left; width:100px; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}
.wrap_ticker .index_area .items {float:left;}
.wrap_ticker .index_area .items li {float:left; width:60px; text-align:right;}
.wrap_ticker .index_area .items li.item1 {margin-right:10px; font-weight:700;}
.wrap_ticker .index_area .items li.item3 {width:50px;}
.wrap_ticker .move_area {float:right;}
.wrap_ticker .move_area:after {display:block;content:'';clear:both;float:none;}
.wrap_ticker .move_area button {float:left; box-sizing:border-box; padding:0; width:14px; height:14px; background:#f4f4f5 url(/resources/images/bg_arrow_sml.png) no-repeat;text-indent:-9999px; border:0;}
.wrap_ticker .move_area button.btn_prev {background-position:5px 50%;}
.wrap_ticker .move_area button.btn_next {margin-left:1px; background-position:-42px 50%;}
.wrap_ticker .move_area button:hover {background-color:#e8e8e8; outline:1px solid #bbb;}

/* rolling component */
.group_wrap {position:relative; float:left; overflow:hidden;}
.group_item {overflow:hidden;}

/* paging */
.pagination {padding-top:8px; text-align:right;}
.pagination a {display:inline-block; box-sizing:border-box; width:23px; height:26px; line-height:26px; border-radius:3px; text-align:center; vertical-align:middle; transition:all 0.2s;}
.pagination a:hover {color:#000; text-decoration:underline;}
.pagination .current {font-weight:700; color:#fff; background:#b7b7b7;}
.pagination .prev,
.pagination .next {border:1px solid #b7b7b7; text-indent:-9999px;}
.pagination .prev {background:url(/resources/images/bg_prev.png) no-repeat -2px 50%;}
.pagination .next {background:url(/resources/images/bg_next.png) no-repeat -2px 50%;}
.pagination .prev:hover,
.pagination .next:hover {border-color:#999; box-shadow:0 0 3px rgba(0,0,0,0.2);}

/* calendar */
/* input.datepicker {padding-right:0; width:63px!important; border-right:0; border-radius:3px 0 0 3px;} */
input.datepicker {width:75px !important; padding-left:10px; border-right:0; border-radius:3px 0 0 3px;}
.ui-datepicker-trigger {width:23px !important; height:20px; background:url(/resources/images/bg_calendar.png) no-repeat 50% !important; text-indent:-999px; border:1px solid #d0d0d0; border-left:0; border-radius:0 3px 3px 0; overflow:hidden;}

.ui-datepicker .ui-datepicker-header {position:relative; padding:0 15px 20px; margin:20px 0 15px; background:#fcfcfc; border:0; border-bottom:1px solid #eaeaea; border-radius:0;}
.ui-datepicker .ui-datepicker-prev {left:15px;}
.ui-datepicker .ui-datepicker-next {right:15px;}
.ui-datepicker.ui-corner-all {display:none; margin:5px 0 0;
    background:#fcfcfc; font-size:11px; border:1px solid #d0d0d0; border-radius:3px; z-index:2;}
.ui-datepicker {width:auto; box-shadow:1px 1px 1px #c6c9cc;}
.ui-datepicker .ui-datepicker-calendar {width:auto; margin:0 15px 15px; border-collapse:separate; border-spacing:6px;}
.ui-datepicker table th {font-size:11px; font-weight:400; border:0; background:transparent; color:#666;}
.ui-datepicker td {padding:0; border:0 !important; background:transparent;}
.ui-datepicker td a {display:block; padding:6px 5px 3px; color:#666;}
.ui-datepicker td.ui-datepicker-today {}
.ui-datepicker td.ui-datepicker-today a {background:#f88a31; color:#fff; border:0; border-radius:8px;}
.ui-datepicker td .ui-state-default {}
.ui-datepicker td:hover a {background:#e5e4e1;}
.ui-datepicker .ui-datepicker-week-end,
.ui-datepicker .ui-datepicker-week-end a {color:#0082ca;}
.ui-datepicker .ui-datepicker-week-end:first-child,
.ui-datepicker .ui-datepicker-week-end:first-child a {color:#ed5432;}
.ui-datepicker .ui-datepicker-next,
.ui-datepicker .ui-datepicker-prev {position:absolute; top:2px; height:1.8em; width:15px; background:url(/resources/images/bg_cal_prev.png) no-repeat 50%; cursor:pointer;}
.ui-datepicker .ui-datepicker-next {background-image:url(/resources/images/bg_cal_next.png);}
.ui-datepicker .ui-datepicker-next span,
.ui-datepicker .ui-datepicker-prev span {display:none;}
.ui-widget-header .ui-icon {background:transparent;}
.ui-datepicker-multi-3 .ui-datepicker-group {width:180px}
#ui-datepicker-div > * {font-size:12px}
.ui-datepicker .ui-datepicker-title {margin:0; color:#4f4e4e; text-align:center;}
.ui-datepicker .ui-datepicker-title select {height:26px; padding:0 8px; vertical-align:middle; border:1px solid #d0d0d0; border-radius:3px;}
.ui-datepicker select.ui-datepicker-month, .ui-datepicker select.ui-datepicker-year {width:60px; margin:0 1px; border:1px solid #cdccca; background-color:#fff;}

/* blocking element */
.blockUI.blockOverlay {opacity:0.7!important; background:#fff!important; z-index:98!important;}
.blockUI.blockMsg {padding-top:30px!important; border:0!important; background:url(/resources/images/spinner.gif) no-repeat 50% 0!important; font-size:12px; z-index:99!important;}

/* tab */
div[name=tabs] {margin-top:15px;}
div[name=tabs]:first-child {margin-top:0;}

.nav_tabs {overflow:hidden;}
.nav_tabs li {position:relative; float:left; box-sizing:border-box; border:1px solid #c9d1d5; height:30px;}
.nav_tabs li + li {margin-left:-1px;}
.nav_tabs li a {display:block; padding:0 10px; height:28px; line-height:28px; color:#828e9f; background:#f3f3f4; font-weight:700; /*2017.03.23추가*/ font-size:12px; font-family: Verdana, Geneva, sans-serif}
.nav_tabs li.ui-tabs-active {border:1px solid #2e65b0; border-bottom:0; z-index:1;}
.nav_tabs li.ui-tabs-active a {background:#fff; color:#2e65b0;}

.modal_wrap .nav_tabs li a {width:70px; text-align:center;}
.modal_wrap .nav_tabs li.ui-tabs-active {border-color:#ed5432;}
.modal_wrap .nav_tabs li.ui-tabs-active a {color:#ed5432;}
.modal_wrap .nav_tabs li.ui-tabs-active:first-child {border-color:#17a668;}
.modal_wrap .nav_tabs li.ui-tabs-active:first-child a {color:#17a668;}

.tab_content {margin-top:8px;}
.tab_content > div {position:relative;}
.tab_content > div:after {content:''; display:block; clear:both;}

/* buttons */
input[type=submit],
input[type=reset],
input[type=button] {display:inline-block; box-sizing:border-box; width:78px; height:26px; line-height:24px; text-align:center; color:#2e65b0; font-weight:700; border:1px solid #2e65b0; border-radius:0; background:#fff; transition:all .2s;}
input[type=submit].color,
input[type=reset].color,
input[type=button].color {background:#2e65b0; color:#fff;}
input[type=submit]:hover,
input[type=reset]:hover,
input[type=button]:hover {box-shadow:0 2px 7px rgba(0,0,0,.3);}
.btn {display:inline-block; height:20px; padding:0 7px; margin:0 0 0 5px; background:#0082ca; color:#fff; font-size:11px; font-weight:600; line-height:20px; letter-spacing:1px; vertical-align:middle; border:0; overflow:hidden; border-radius:3px; transition:all .2s;}
.btn:hover {background:#afafaf; color:#fff; text-decoration:none; box-shadow:0 1px 5px rgba(0,0,0,.3);}
.btn.buy {background:#17a668;}
.btn_refresh {margin-top:-2px;width:18px;height:16px; padding:0; background:transparent url(/resources/images/ico_refresh6.png) no-repeat 50% 4px; vertical-align:middle;}
.btn_refresh:hover {border-color:#c9d1d5;}
.btn_rescan {position:relative; display:inline-block; padding:0 0 0 18px;height:20px;line-height:20px;}
.btn_rescan:after {content:''; position:absolute; top:3px; left:0; display:block; width:15px; height:15px; background:url(/resources/images/bg_rescan.png) no-repeat 0 50%;}
.btn_rescan:hover {color:#000;}
.btn_rescan:hover:after {transition:transform .5s ease; transform:rotate(360deg);}
.btn_down {margin-left:10px; width:25px; height:20px; background:url(/resources/images/bg_down.png) no-repeat 50% 50%; border:0; text-indent:-9999px;}
.btn_down:hover {background-position:50% -83px;}
.btn_del {width:20px; height:14px; background:url(/resources/images/bg_del.png) no-repeat 50% 3px; text-indent:-9999px; border:0;}

/* button set */
.button_set {display:inline-block; border:1px solid #d0d0d0; border-radius:3px; overflow:hidden; vertical-align:middle;}
.button_set label {float:left; margin:0; padding:0 5px; font-weight:400; height:18px; line-height:18px; border-left:1px solid #d0d0d0; cursor:pointer;}
.button_set input:first-child + label {border:0;}
.button_set input {position:absolute; visibility:hidden; z-index:-1;}
.button_set input:checked + label {color:#343434; background:#f9f9f9;}

/* dropdown element */
.input_dropdown {position:relative; display:inline-block; vertical-align:middle;}
.input_dropdown > span {display:block; overflow:hidden;}
.input_dropdown > span input[type=text] {float:left; width:80px; border-right:0; border-radius:3px 0 0 3px;}
.input_dropdown > span button {float:left; width:23px; height:20px; background:url(/resources/images/bg_select.png) no-repeat 50% 50%; text-indent:-9999px; border:1px solid #d0d0d0; border-radius:0 3px 3px 0; overflow:hidden;}
.input_dropdown div {position:absolute; top:19px; left:0; box-sizing:border-box; width:100%; max-height:150px; border:1px solid #d0d0d0; border-radius:3px; background:#fff; overflow-x:hidden; overflow-y:auto;}
.input_dropdown div a {display:block; padding:3px 5px; text-align:left; color:#666;}
.input_dropdown div a span {font-weight:700; color:#343434;}
.input_dropdown div a:hover {background:#f0f0f0;}

/* recent input search */
.input_search {position:relative; display:inline-block; vertical-align:middle;}
label + .input_search,
.input_search label {margin-right:4px;}
.input_search input {float:left; border-radius:3px 0 0 3px;}
.input_search button {float:left; width:23px; height:20px; margin:0 0 0 -1px; background:url(/resources/images/bg_search.png) no-repeat 50%; font-size:0; line-height:0; border:1px solid #d0d0d0; border-radius:0 3px 3px 0; overflow:hidden;}
.input_search .btn_recent {position:absolute; right:24px; top:1px; width:20px; height:18px; background:#fff url(/resources/images/bg_select.png) no-repeat 50% 50%; border:0;}
.input_search + input {width:150px; margin:0 0 0 10px;}

.layer_newest {position:absolute; left:0; top:19px; padding:5px 0; width:155px; height:180px; background:#fff; font-size:11px; color:#888; border:1px solid #d0d0d0; overflow-y:auto; z-index:10; border-radius:3px;}
.layer_newest li {padding:5px 8px; font-size:11px;}
.layer_newest li ol,
.layer_newest li ul {margin:7px 0 0; border-top:1px dashed #d0d0d0;}
.layer_newest li li {color:#343434;}
.layer_newest ul li:hover,
.layer_newest li li:hover {background:#f9f9f9;}

/* search bar */
.search_area {position:relative; margin-top:15px;}
.search_area:first-child,
h2 + .search_area,
h3 + .search_area {margin-top:0;}
.search_area:after {display:block;content:'';clear:both;float:none;}
.search_area h3 {float:left;}
.search_area label:after {content:''; position:absolute; top:10px; left:0; width:1px; border-top:1px solid #7a7d82;}
.search_area button.outline {width:auto; padding-right:5px; line-height:18px; border-left:1px solid #c9d1d5;}
.search_area .datepicker {margin:0 0 0 4px;}
.search_area .pull_right .total {float:left; padding:8px 5px 0 15px;}
.search_area .confirm {margin-left:6px; width:60px; height:20px; line-height:20px; text-align:center; background:#0082ca; border:0; font-weight:700; color:#fff; border-radius:3px; transition:all .2s;}
.search_area .confirm:hover {box-shadow:0 2px 6px rgba(0,0,0,.3);}
.search_area .input_dropdown span input {width:90px; background-color:#fff;}
.search_area .input_dropdown span button {background-color:#fff;}
.search_area.search_table {float:none; border-width:1px 0 0 0;}
.search_area.search_table .datepicker,
.search_area.search_table .ui-datepicker-trigger {background-color:#fff;}

/* table default */
caption {padding:10px; background:#f9f9f9; border-bottom:1px solid #c9d1d5; background-color:#003763; color: #fff;}
table {width:100%;}
table th,
table td {padding:4px; height:16px; border:1px solid #d7d7d7; border-width:0 1px 1px 1px; white-space:nowrap;}
table th {color:#fff; background-color:#00457c; font-weight:normal; border-color:#c9d1d5;}
table thead th {text-align:center;}
table td:first-child,
table th:first-child {border-left: 0;}
table th.multi_line {padding-top:4px; height:41px; line-height:1.6;}
table th label {color:#fff;}

table td {background-color:#fff; vertical-align:middle; text-align:right;}
table td.dotted {border-left:1px dashed #d0d0d0;}
table td.dotted_bt {border-bottom:1px dashed #d0d0d0;}
table td.has_input {padding-top:2px; padding-bottom:2px;}
table td.text_subject {padding-right:5px; text-align:left !important; text-overflow:ellipsis; white-space:nowrap; overflow:hidden;}

table tbody th {padding-top:2px; font-weight:normal; text-align:left; line-height:14px;}
table tfoot th {padding-right:10px; background:#f9fafb; color:#666; text-align:right;}
table tfoot td {border-bottom:1px solid #c9d1d5;text-align:right;}
table tfoot tr:first-child td {border-color:#c9d1d5;}

table td a {color:#0070c9;}
table td a:hover {text-decoration:underline;}
table td select,
table td input[type=text],
table td input[type=text] {width:100%;}
table label {line-height:1; height:auto;}
table input,
table select,
table .btn,
table .ui-datepicker-trigger,
table .input_dropdown {margin-top:-2px; margin-bottom:-2px;}
table .input_dropdown input {margin:0;}


.selected th,
.selected td {background-color:#f0f0f0;}
.even td {background-color:#fcfcfc;}
.disabled {background:url(/resources/images/bg_disable.png);}

table.no_bbt {border-collapse:separate;}
.no_bbt tbody tr:last-child th,
.no_bbt tbody tr:last-child td {border-bottom:0;}
.no_bbt tfoot th,
.no_bbt tfoot td {border-bottom:0; border-top-width:1px;}

/* table td dark background */
.dark table th {background-color:#00457c; color:#fff;}
.dark table td {background-color:#000000; color:#fff; font-weight: bold;}
/* .dark table td.matching {background-color:#3a3e3a!important; color:#fff; font-weight: bold;} */
.dark table td.c_code {color:#fff;text-align:center !important;}
.dark table label {line-height:1; height:auto; color:#fff!important;}
.dark .even td {background-color:#000;}

/* table wrapper */
.group_table {margin-top:8px; border:1px solid #c9d1d5; border-radius:3px;}
.group_table:first-child {margin-top:0;}
.group_table + .group_table {margin-top:15px;}
.group_table.center td {text-align:center !important;}
.group_table.double td div,
.group_table.double td p {padding-top:4px;}
.group_table.double td > div:first-child,
.group_table.double td > p:first-child {padding:0 0 4px 0; border-bottom:1px dashed #d7d7d7;}
.group_table .btn_s {padding:0; background:transparent; border:0;}

/* table type1 */
.group_table.type1 td {border-bottom-style:dashed;}

/* for fixed th table in layer popup */
.table_outer {position:relative; padding:24px 0 0; background:#f9f9f9; border:1px solid #c9d1d5; border-radius:3px;}
.table_outer caption {display:none;}
.table_outer table {table-layout:fixed;}
.table_outer table th {box-sizing:border-box; height:24px;}
.table_outer .group_table {height:200px; background:#fff; border:0; overflow-y:scroll;}
.table_outer .group_table thead tr {position:absolute; left:0; top:0;}
.table_outer .group_table thead th {width:248px;}
.table_outer .group_table thead th:first-child {width:80px;}

/* right top positioned element for table */
.table_right {position:absolute; top:-28px; right:0;}
.table_right label,
.table_right .btn {margin-left:0;}
.table_right #mark,
.table_right #updn {width:75px;}

/* scroll wrapper */
.grid_area {color:#4f4e4e; overflow:auto;}
.grid_area {margin-top:8px; border:1px solid #c9d1d5; border-radius:3px;}
.grid_area:first-child {margin-top:0;}
.mdi_bottom + .grid_area,
.grid_area + .grid_area {margin-top:15px;}
.grid_area .group_table {margin:0; border:0;}

/* description text */
.desc {margin-top:10px;}
.desc p {position:relative; padding-left:10px; line-height:1.2;}
.desc p:after {position:absolute; top:6px; left:0; content:''; display:block; width:4px; border-top:1px solid #4f4e4e;}
.desc p + p {margin-top:3px; }
.desc .title {padding-left:0; color:#888; font-size:12px; font-weight:700;}
.desc .title:after {display:none;}

/* alert layer */
.layer_match {position:fixed; bottom:10px; right:20px; padding:15px; border:1px solid #2e65b0; border-radius:3px; background:#fff; box-shadow:1px 1px 3px rgba(0,0,0,.2); z-index:99;}

/* loading full cover */
.loading_cover {display:none;}
.loading {position:absolute; left:50%; top:50%; width:80px; height:80px; margin:-40px 0 0 -40px; background:url(/resources/images/loading.gif) no-repeat 50% 50%; background-size:100%; text-indent:-999px; overflow:hidden; z-index:102;}

/* layout */
/*.wrapper {margin:0 auto; width:970px;}*/
.wrapper {margin:0 auto; width:1120px;}
.flexible .wrapper {margin:0 30px; width:auto; min-width:1200px;}
.mdi_container {position:relative; margin-top:15px; min-height:692px;}

.pull_right {float:right !important;}
.pull_left {float:left !important;}
.pull_left + .pull_left {margin:0 0 0 10px;}

.wrap_left {float:left;}
.wrap_right {float:right;}

.mdi_bottom {padding:8px 0 0; text-align:center;}
.mdi_bottom.right {text-align:right;}
.mdi_bottom.left {text-align:left;}

h3 {height:20px; line-height:30px; font-weight:600;}
.cash_wrap + h3,
.grid_area + h3,
.group_table + h3 {margin-top:15px;}

