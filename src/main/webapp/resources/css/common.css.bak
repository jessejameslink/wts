@charset "utf-8";
/* html5doctor.com Reset v1.6.1 - http://cssreset.com */
html,body,div,span,object,iframe,h1,h2,h3,h4,h5,h6,p,blockquote,pre,abbr,address,cite,code,del,dfn,em,
img,ins,kbd,q,samp,small,strong,sub,sup,var,b,i,dl,dt,dd,ol,ul,li,fieldset,form,label,legend,
table,caption,tbody,tfoot,thead,tr,th,td,article,aside,canvas,details,figcaption,figure,footer,header,hgroup,
menu,nav,section,summary,time,mark,audio,video,input{margin:0;padding:0;border:0;outline:0; font-style:normal; background:transparent;}
body,input,textarea,select,button,table{color:#666; font-family:Arial, Helvetica, sans-serif; font-size:12px;}
td{font-size:12px;}

a {color:#666;text-decoration:none}
a:hover {color:#666;}

ul,ol,li {list-style:none}
article,aside,details,figcaption,figure,footer,header,hgroup,menu,nav,section{display:block}
input,button,select {box-sizing:border-box; vertical-align:middle;}

blockquote,q{quotes:none}
blockquote:before,blockquote:after,q:before,q:after{content:none}
a{margin:0;padding:0;font-size:100%;vertical-align:baseline;background:transparent}
ins{background-color:#ff9;color:#000;text-decoration:none}
mark{background-color:#ff9;color:#000;font-style:italic;font-weight:bold}
del{text-decoration:line-through}abbr[title],dfn[title]{border-bottom:1px dotted;cursor:help}
table{border-collapse:collapse;border-spacing:0}
hr{display:block;height:1px;border:0;border-top:1px solid #ccc;margin:1em 0;padding:0}

/* common component */
body {background:#fff;}
input[type=text],
input[type=password],
select {height:20px; padding:0 5px; border:1px solid #d7d7d7; border-radius:3px;}
input[type=submit],
input[type=reset],
input[type=button],
button {cursor:pointer;}
input[type=radio],
input[type=checkbox],
select {
    -webkit-appearance:none;
    -moz-appearance:none;
    -ms-appearance:none;
    -o-appearance:none;
    appearance:none;
}
input[type=radio] {display:inline-block; width:12px; height:12px; background:url(/resources/images/bg_radio.png) no-repeat;}
input[type=radio]:checked {background-position:0 -12px;}
input[type=checkbox] {display:inline-block; width:13px; height:13px; background:url(/resources/images/bg_check.png) no-repeat;}
input[type=checkbox]:checked {background-position:0 -13px;}
input.won {text-align:right;}
/*input[readonly] {background-color:#fefcfc; border-color:#e9e9e9;}*/
input[readonly] {background-color:transparent; border-color:transparent;}

select::-ms-expand {display: none;}
/* select {padding-right:25px;background:url(/resources/images/bg_select.png) no-repeat 100% 50%;} */
select {padding-right:25px;background:url(/resources/images/bg_select02.png) no-repeat 100% 50%;}

label {position:relative; display:inline-block; margin:0 2px 0 10px; padding-left:5px; height:20px; line-height:20px; vertical-align:middle; color:#999;}
label:first-child {margin-left:0;}
input + label {margin:0 10px 0 0;}
input + label:after {display:none;}
