<?xml version='1.0' encoding='utf-8'?>

<!--   거래량 순위  -->
<transaction trcode="piboindu" servicecode=""  type="row">
	
	<value io="in"		name="symb"		type="string"		length="12"	comment="Search key "/>
	<value io="in"		name="smkt"		type="string"		length="4"	comment="Market "/>
	<value io="in"		name="pinx"		type="string"		length="4"	comment="Page Index "/>
	<value io="in"		name="psiz"		type="string"		length="4"	comment="Page Size "/>
	
	<value io="out"		name="nrec"		type="string"		length=" 4"	comment="# of Record"/>
		
	<table name="list1" count="nrec">
		<value io="out"	name="row"				type="string"	length="5"		comment="Row"/>
		<value io="out"	name="stockcode"		type="string"	length="12"		comment="Stock symbol"/>
		<value io="out"	name="stockname"		type="string"	length="64"		comment="Stock name"/>
		<value io="out"	name="lastprice"		type="string"	length="9"		comment="Last price"/>
		<value io="out"	name="perchange"		type="string"	length="7"		comment="Perchange"/>
		<value io="out"	name="highprice"		type="string"	length="9"		comment="High price"/>
		<value io="out"	name="lowprice"			type="string"	length="9"		comment="Low price"/>
		<value io="out"	name="totalvol"			type="string"	length="9"		comment="Total Vol"/>
		<value io="out"	name="totalval"			type="string"	length="18"		comment="Total Val"/>
		<value io="out"	name="fbuyvol"			type="string"	length="9"		comment="F Buy Vol"/>
		<value io="out"	name="fsellvol"			type="string"	length="9"		comment="F Sell Vol"/>
		<value io="out"	name="mkcap"			type="string"	length="18"		comment="Mk Cap"/>
		<value io="out"	name="pe"				type="string"	length="18"		comment="PE"/>
		<value io="out"	name="pb"				type="string"	length="20"		comment="PB"/>
		<value io="out"	name="color"			type="string"	length="2"		comment="Color"/>		
	</table>
	
</transaction>
